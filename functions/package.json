{
  "name": "saaba-functions",
  "description": "saaba cloud functions",
  "author": "musakui",
  "license": "MIT",
  "version": "0.0.1",
  "private": "true",
  "engines": {
    "node": ">=14.0.0"
  },
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c --input",
    "build:auth": "npm run build src/auth.js",
    "build:info": "npm run build src/info.js",
    "dev": "nodemon -r dotenv/config src/index.js",
    "framework": "env-cmd functions-framework --target",
    "deploy": "gcloud functions deploy --env-vars-file=.env.yaml",
    "logs": "gcloud functions logs read",
    "list": "gcloud functions list"
  },
  "dependencies": {
    "@google-cloud/compute": "^2.6.0",
    "@google-cloud/firestore": "^4.13.2",
    "@google-cloud/functions-framework": "^1.9.0",
    "@google-cloud/secret-manager": "^3.8.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.0.2",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.12",
    "rollup": "^2.53.2",
    "rollup-plugin-terser": "^7.0.2"
  }
}
